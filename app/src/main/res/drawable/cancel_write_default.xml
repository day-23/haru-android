<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#707070" />
            <size
                android:width="77dp"
                android:height="77dp" />
        </shape>
    </item>
    <item android:bottom="20dp"
        android:top="20dp"
        android:end="20dp"
        android:start="20dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="line">
                <stroke
                    android:color="#FFFFFF"
                    android:width="4dp" />
                <size
                    android:width="20dp"
                    android:height="1dp" />
                <path android:pathData="M0,0 L20,0" />
            </shape>
        </rotate>
    </item>
    <item android:bottom="20dp"
        android:top="20dp"
        android:end="20dp"
        android:start="20dp">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="line">
                <stroke
                    android:color="#FFFFFF"
                    android:width="4dp" />
                <size
                    android:width="20dp"
                    android:height="1dp" />
                <path android:pathData="M0,0 L20,0" />
            </shape>
        </rotate>
    </item>
</layer-list>
